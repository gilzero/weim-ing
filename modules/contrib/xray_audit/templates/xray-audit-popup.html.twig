{#
/**
 * @file
 * Default theme implementation for an export icon.
 *
 * Available variables:
 * - url: An internal system path of the data export.
 * - attributes: Remaining HTML attributes for the export link.

 *
 * @ingroup themeable
 */
#}
{% if iframe_url %}
<iframe src="{{ iframe_url }}"{{ attributes }}></iframe>
{% else %}
  {% if attributes %}
    <div{{ attributes }}>
  {% endif %}
  {{ content }}
  {% if attributes %}
    </div>
  {% endif %}
{% endif %}
