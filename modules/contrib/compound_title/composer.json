{
  "name": "drupal/compound_title",
  "type": "drupal-module",
  "description": "Provides a simple compound title field type.",
  "keywords": ["Drupal"],
  "license": "GPL-2.0+",
  "homepage": "https://www.drupal.org/project/compound_title",
  "support": {
    "issues": "https://www.drupal.org/project/issues/compound_title",
    "source": "https://git.drupalcode.org/project/compound_title"
  },
  "require": {
    "php": ">=8.0",
    "ext-dom": "*",
    "drupal/core": "^9.5 || ^10 || ^11"
  },
  "authors": [
    {
      "name": "Bogdan Racz (bogdan.racz)",
      "email": "bogdan.racz@dreamproduction.com"
    }
  ],
  "require-dev": {
    "drupal/coder": "^8.3",
    "phpcompatibility/php-compatibility": "^9.3"
  },
  "minimum-stability": "dev",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
