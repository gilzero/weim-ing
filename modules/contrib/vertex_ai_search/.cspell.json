{
    "description": "This default cspell configuration for contrib projects is based on core/.cspell.json. See https://project.pages.drupalcode.org/gitlab_templates/jobs/cspell/ for more details.",
    "language": "en-US",
    "allowCompoundWords": false,
    "globRoot": ".",
    "minWordLength": 4,
    "ignorePaths": [
        "web",
        "vendor",
        "node_modules",
        ".git",
        "**/.*.json",
        "package.json",
        "yarn.lock",
        "phpstan*",
        ".*ignore",
        "./composer.lock",
        "./composer.json"
    ],
    "dictionaries": [
        "companies",
        "fonts",
        "html",
        "php",
        "softwareTerms",
        "misc",
        "typescript",
        "node",
        "css",
        "bash",
        "filetypes",
        "npm",
        "lorem-ipsum",
        "drupal",
        "dictionary",
        "project-words"
    ],
    "dictionaryDefinitions": [
        {
            "name": "drupal",
            "path": "web/core/misc/cspell/drupal-dictionary.txt"
        },
        {
            "name": "dictionary",
            "path": "web/core/misc/cspell/dictionary.txt"
        },
        {
            "name": "project-words",
            "path": "./.cspell-project-words.txt",
            "description": "The project's own custom dictionary (optional)"
        }
    ],
    "words": [
        "vertex_ai_search",
        "vertex",
        "ai",
        "search",
        "lando",
        "ddev"
    ],
    "flagWords": [
        "e-mail",
        "grey",
        "queuing"
    ]
}